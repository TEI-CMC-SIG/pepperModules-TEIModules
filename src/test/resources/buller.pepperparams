<?xml version="1.0" encoding="UTF-8"?>
<pepper-job>
  <importer name="TEIImporter" path="./buller">
	<customization>
		<property key="SubTokenization">true</property>
		<property key="ForeignAsToken">true</property>
		<property key="UnclearAsToken">true</property>
	</customization>
  </importer>
  <exporter name="RelANNISExporter" path="./target/bullerOutput">
	<customization>
		<property key="relANNIS.exporter.linebreak_escape">BLANK</property>
		<property key="relANNIS.exporter.tab_escape">TAB</property>
		
	</customization>
  </exporter>
</pepper-job>
